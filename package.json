{
  "private": true,
  "scripts": {
    "dev": "(npm run frontend && npm run backend)",
    "prod": "(npm run frontend_prod && npm run backend_prod)",
    "backend": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --env.part=backend --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "frontend": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --env.part=frontend --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "backend_prod": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --env.part=backend --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "frontend_prod": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --env.part=frontend --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "devDependencies": {
    "axios": "^0.17",
    "bootstrap": "^4.0.0-beta.2",
    "cross-env": "^5.1",
