    "font-awesome": "^4.7.0",
    "imports-loader": "^0.7.1",
    "inputmask": "^3.3.7",
    "jquery": "^3.3.1",
    "laravel-mix": "^2.0.0",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "lodash": "^4.17.19",
    "pace": "github:HubSpot/pace#v1.0.2",
    "popper.js": "^1.12.4",
    "select2": "^4.0.3",
    "simple-line-icons": "^2.4.1",
    "sweetalert2": "^7.7.0",
